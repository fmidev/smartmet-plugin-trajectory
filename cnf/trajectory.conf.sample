url		= "/trajectory";

timestep  	= 10;		// minutes
length		= 24;		// hours
format		= "kml";	// gpx, kml, kmz, kmlx, kmzx, xml, debug
pressure	= 850;		// hPa

templates	= "/usr/share/smartmet/trajectory/";

plume:
{
	disturbance	= 25;	// perturbation percentage
	range		= 0;	// perturbation hPa
	radius		= 0;	// perturbation km
	interval	= 0;	// perturbation minutes
};

kml:
{
	tessellate	= false;
	extrude		= false;
};
